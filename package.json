{
  "name": "gpt_chrome",
  "version": "1.0.0",
  "description": "GPT helper extension ",
  "main": "dev-server.js",
  "scripts": {
    "hot": "node dev-server.js",
    "build-test": "webpack --config build/webpack.config.js --progress --watch --mode=development",
    "prehot": "npm run prebuild-popup",
    "prebuild-scripts": "find extension/dist | grep -e background.js -e content_script.js -e inject_script.js | xargs rm",
    "build-scripts": "webpack --config webpack.config.js --env.BUILD_TYPE=scripts --progress --colors",
    "build-popup": "webpack --config webpack.config.js --env.BUILD_TYPE=popup --progress --colors",
    "prebuild-popup": "find extension/dist | grep popup.js | xargs rm",
    "build-scripts-prod": "npm run build-scripts -- --env.NODE_ENV=production",
    "build-popup-prod": "npm run build-popup -- --env.NODE_ENV=production",
    "build-all-prod": "npm run build-scripts-prod && npm run build-popup-prod"
  },
  "author": "Matt Bedell",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "on-build-webpack": "^0.1.0",
    "react-hot-loader": "^3.1.3",
    "redux-logger": "^3.0.6",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "file-loader": "^1.1.5",
    "polished": "^1.9.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-transition-group": "^2.2.1",
    "redux": "^3.6.0",
    "styled-components": "^2.4.0",
    "webpack": "^4.20.2",
    "webpack-merge": "^4.1.1"
  }
}
